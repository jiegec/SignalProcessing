function res = overlap_add(x, h, L)
    N = length(h)+L-1;
    res = zeros(1, N);
    i = 1;
    fft_h = fft(h, N);
    while i <= length(x)
        to = min(i + L - 1, length(x));
        part = ifft(fft(x(i:to), N) .* fft_h);
        to = min(i + N - 1, length(x) + length(
        res(i:to) = res(i:to) + part(1:
        i = i + L;
    end
end